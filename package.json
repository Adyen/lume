    "jest-environment-jsdom": "^29.3.1",
    "jsdom": "^20.0.3",
    "mini-css-extract-plugin": "^2.7.0",
    "prettier": "^2.7.1",
    "resize-observer-polyfill": "^1.5.1",
    "sass": "^1.43.4",
