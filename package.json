{
  "name": "@adyen/lume",
  "version": "0.1.0",
  "description": "Lume is a component library for visual representations of data, built for Vue with D3.",
  "private": true,
  "main": "dist/index.umd.js",
  "module": "dist/index.esm.js",
  "files": [
    "dist/"
  ],
  "exports": {
    ".": "./dist/index.esm.js",
    "./styles": "./dist/styles/main.css",
    "./font": "./dist/styles/font.css",
    "./scss": "./dist/scss/main.scss",
    "./charts/*": "./dist/charts/*.js",
    "./core/*": "./dist/core/*.js",
    "./groups/*": "./dist/groups/*.js",
    "./package.json": "./package.json"
  },
  "types": "./dist/types/index.d.ts",
  "license": "MIT",
  "contributors": [
    {
      "name": "Govind Srinidhi",
      "email": "govind.srinidhi@adyen.com"
    },
    {
      "name": "Joao Santos",
      "email": "joao.dossantos@adyen.com"
    },
    {
      "name": "Lucas van Heerikhuizen",
      "email": "lucas.vanheerikhuizen@adyen.com"
    },
    {
      "name": "Vivian Joseph",
      "email": "vivian.joseph@adyen.com"
    }
  ],
  "scripts": {
    "build:es": "webpack --config build/webpack.prod.es.js --progress",
    "build:umd": "webpack --config build/webpack.prod.umd.js --progress",
    "build": "NODE_ENV=production npm run build:es",
    "confighooks": "git config core.hooksPath ./git-hooks",
    "dev": "webpack --config build/webpack.dev.js --progress",
    "lint:fix": "npm run lint -- --fix",
    "lint:pretty": "prettier --write --ignore-unknown .",
    "lint": "eslint ./src ./test --ext .js,.ts,.vue",
    "prepare": "npm run confighooks && echo \" âœ” Git hooks path configured to ./git-hooks\"",
    "release:major": "standard-version --release-as major",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release": "standard-version",
    "start": "npm run storybook",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "unit-exp:update:snapshot": "npm run unit-exp -- -u",
    "unit-exp": "NODE_OPTIONS=\"--experimental-vm-modules\"; jest --config=test/config/jest.config.js",
    "unit": "jest --config=test/config/jest.config.js"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.4",
    "d3": "^6.6.2",
    "d3-sankey": "^0.12.3"
  },
  "peerDependencies": {
    "vue": "^2.7.8"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^6.5.4",
    "@storybook/addon-essentials": "^6.5.4",
    "@storybook/addon-storysource": "^6.5.4",
    "@storybook/addons": "^6.5.4",
    "@storybook/builder-webpack5": "^6.5.4",
    "@storybook/manager-webpack5": "^6.5.4",
    "@storybook/theming": "^6.5.4",
    "@storybook/vue": "^6.5.4",
    "@types/d3-format": "^3.0.1",
    "@types/d3-sankey": "^0.11.2",
    "@types/d3-scale": "^4.0.2",
    "@types/d3-shape": "^3.1.0",
    "@types/jest": "^27.5.0",
    "@types/node": "^17.0.35",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "@vue/test-utils": "^1.1.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^28.1.2",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^3.6.0",
    "eslint": "^8.12.0",
    "eslint-plugin-vue": "^9.2.0",
    "file-loader": "^6.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^28.1.2",
    "jest-environment-jsdom": "^28.0.2",
    "mini-css-extract-plugin": "^2.5.3",
    "prettier": "^2.7.1",
    "resize-observer-polyfill": "^1.5.1",
    "sass": "^1.43.4",
    "sass-loader": "^8.0.2",
    "standard-version": "^9.3.2",
    "storybook-addon-designs": "^6.2.1",
    "ts-jest": "^28.0.5",
    "typescript": "^4.6.3",
    "vue-hot-reload-api": "^2.3.4",
    "vue-jest": "^3.0.7",
    "vue-loader": "^15.10.0",
    "vue-style-loader": "^4.1.3",
    "vue-template-compiler": "^2.7.8",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.10.0",
    "webpack-merge": "^5.8.0"
  },
  "keywords": [
    "Lume",
    "LumeJS",
    "Lume Design System",
    "Data Visualization",
    "Charts",
    "d3.js",
    "SVG charts",
    "Bar charts",
    "Single bar chart",
    "Grouped bar chart",
    "Stacked bar chart",
    "Sparkline chart",
    "Line chart",
    "Alluvial charts",
    "Sankey diagrams",
    "Vue chart library",
    "Vue",
    "Vue 2.7"
  ]
}
